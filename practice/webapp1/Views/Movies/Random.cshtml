@model IEnumerable<webapp1.Models.Movies>
@{
    ViewBag.Title = "Random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h4>User Details</h4>
 @if (Model != null)
 {
        <table>
            <tr>
                <th>isbn_13</th>
                <th>Title</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(modelitem => item.ISBN_13) </td>
                    <td>@Html.DisplayFor(modelitem => item.Title)</td>
                </tr>
            }
        </table>
 }